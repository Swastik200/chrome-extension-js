{
    "manifest_version": 2,
    "name": "RaterFox",
    "description": "Watch new TV series on Netflix",
    "version": "1",
    "author": "Jake Prins",
    "browser_action": {
    "default_icon": "tab-icon.png",
    "default_title": "Have a good day"
    },
    "chrome_url_overrides" : {
    "newtab": "newtab.html"
    },
    "permissions": ["activeTab"] 
    }