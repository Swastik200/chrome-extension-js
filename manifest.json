{
    "manifest_version": 2,
    "name": "test",
    "description": "Watch new TV series on Netflix",
    "version": "1",
    "author": "Jake Prins",

  "browser_action": {

    "default_popup": "newtab.html",
    "default_title": "Open the popup"
  },
    "permissions": [
      "activeTab",
      "tabs",
      "webNavigation",
      "scripting",
      "bookmarks",
      "cookies"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "options_page": "options.html",
    "omnibox": { "keyword" : "nt" }

 
    }